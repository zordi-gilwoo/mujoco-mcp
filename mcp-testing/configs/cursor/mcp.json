{
  "mcpServers": {
    "mujoco-mcp": {
      "command": "python",
      "args": ["-m", "mujoco_mcp"],
      "env": {
        "PYTHONUNBUFFERED": "1",
        "MUJOCO_MCP_LOG_LEVEL": "INFO",
        "MUJOCO_GL": "egl"
      },
      "cwd": "${workspaceFolder}",
      "timeout": 45000,
      "restart": true
    }
  },
  "cursor": {
    "mcp": {
      "enabled": true,
      "autoStart": true,
      "showInStatusBar": true
    }
  }
}